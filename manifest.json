{
    "name": "smartWindow",
    "description": "smartWindow",
    "version": "1.0",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "css": [],
            "js": [
                "jquery.min.js",
                "content.js"
            ],
            "matches": ["*://*/*"],
            "all_frames": true
        }
    ],
    "permissions": [
        "storage",
        "*://*/*",
        "<all_urls>",
        "tabs",
        "activeTab"
    ],
    "web_accessible_resources": [
        "icon.png"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "smartWindow",
        "default_icon": "icon.png"
    },
    "manifest_version": 2
}
