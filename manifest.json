{
  "name": "smartWindow",
  "description": "smartWindow",
  "version": "1.0",
  "icons": {
      "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "smartWindow",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
